opam-version: "2.0"
name: "sensors"
version: "~dev"
depends: [
  "angstrom" {= "0.15.0"}
  "base" {= "v0.14.1"}
  "base-bigarray" {= "base"}
  "base-bytes" {= "base"}
  "base-threads" {= "base"}
